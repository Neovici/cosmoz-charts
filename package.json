{
  "name": "@neovici/cosmoz-charts",
  "version": "1.5.0",
  "description": "Create beautiful charts using billboard.js",
  "keywords": [
    "charts",
    "web-components"
  ],
  "homepage": "https://github.com/Neovici/cosmoz-charts#readme",
  "bugs": {
    "url": "https://github.com/Neovici/cosmoz-charts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neovici/cosmoz-charts.git"
  },
  "license": "Apache-2.0",
  "author": "Cristian Necula <virusz@gmail.com>",
  "main": "cosmoz-chart.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "analyze": "polymer analyze --input cosmoz-*.js  > analysis.json",
    "lint": "eslint --cache --ext .js,.html . && polymer lint cosmoz-*.js",
    "start": "polymer serve -o",
    "test": "polymer test"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@polymer/polymer": "^3.3.0",
    "billboard.js": "^1.10.1",
    "d3": "^5.9.7",
    "d3-scale-chromatic": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@neovici/eslint-config": "github:neovici/eslint-config#semver:^1.0.0",
    "@neovici/github-actions-eslint-annotator": "^0.3.0",
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.19",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@polymer/test-fixture": "^4.0.2",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-mocha": "^5.3.0",
    "husky": "^3.0.9",
    "mocha": "^6.2.2",
    "polymer-cli": "^1.9.11",
    "semantic-release": "^16.0.0-beta.24",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "wct-istanbul": "^0.14.3",
    "wct-mocha": "^1.0.1"
  }
}
