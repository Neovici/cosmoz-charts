{
  "name": "@neovici/cosmoz-charts",
  "version": "2.0.1",
  "description": "Create beautiful charts using billboard.js",
  "keywords": [
    "charts",
    "web-components"
  ],
  "homepage": "https://github.com/Neovici/cosmoz-charts#readme",
  "bugs": {
    "url": "https://github.com/Neovici/cosmoz-charts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neovici/cosmoz-charts.git"
  },
  "license": "Apache-2.0",
  "author": "Cristian Necula <virusz@gmail.com>",
  "main": "cosmoz-chart.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "cosmoz-*.js"
  ],
  "scripts": {
    "lint": "eslint --cache --ext .js .",
    "lint-tsc": "tsc",
    "start": "npm run storybook",
    "test": "wtr --coverage",
    "test:watch": "wtr --watch",
    "storybook": "start-storybook --node-resolve --watch --open",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "prepare": "husky install"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "billboard.js": "^3.0.0",
    "d3-format": "^3.0.0",
    "d3-scale-chromatic": "^3.0.0",
    "d3-time-format": "^4.0.0",
    "haunted": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@neovici/eslint-config": "^1.3.0",
    "@open-wc/demoing-storybook": "^2.4.5",
    "@open-wc/testing": "^2.5.29",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@storybook/storybook-deployer": "^2.8.10",
    "@web/test-runner": "^0.13.0",
    "@web/test-runner-selenium": "^0.5.0",
    "husky": "^7.0.0",
    "semantic-release": "^18.0.0",
    "sinon": "^12.0.0",
    "typescript": "^4.0.0"
  }
}
