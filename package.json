    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-mocha": "^6.3.0",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "polymer-cli": "^1.9.11",
    "semantic-release": "^17.0.4",
