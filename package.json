{
  "name": "@neovici/cosmoz-charts",
  "version": "1.5.1",
  "description": "Create beautiful charts using billboard.js",
  "keywords": [
    "charts",
    "web-components"
  ],
  "homepage": "https://github.com/Neovici/cosmoz-charts#readme",
  "bugs": {
    "url": "https://github.com/Neovici/cosmoz-charts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neovici/cosmoz-charts.git"
  },
  "license": "Apache-2.0",
  "author": "Cristian Necula <virusz@gmail.com>",
  "main": "cosmoz-chart.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint --cache --ext .js,.html .",
    "start": "es-dev-server",
    "test": "polymer test",
    "storybook": "start-storybook --node-resolve --watch --open",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@storybook/storybook-deployer": "^2.8.6",
    "billboard.js": "^1.11.1",
    "d3": "^5.15.0",
    "d3-scale-chromatic": "^1.5.0",
    "haunted": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@neovici/eslint-config": "^1.2.1",
    "@open-wc/demoing-storybook": "^2.4.2",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "es-dev-server": "^1.57.5",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "semantic-release": "^17.0.0",
    "sinon": "^9.0.0",
    "typescript": "^4.0.0"
  }
}
